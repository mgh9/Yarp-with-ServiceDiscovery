{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },

  "ConsulServiceDiscovery": {

    "ConsulClient": {
      "Host": "http://127.0.0.1:8500",
      "Datacenter": ""
    },

    "ServiceRegistration": {
      "Name": "Api 01",
      "Address": "https://192.168.12.112",
      "Port": 7094,

      "Meta": {
        "yarp_is_enabled": "true",
        "yarp_route_match_path": "/api01/{**remainder}",
        "yarp_route_transform_path": "{**remainder}",

        "service_health_endpoint": "/healthz",
        "service_health_check_seconds": 5,
        "service_health_timeout_seconds": 10,
        "service_health_deregister_seconds": 30
      },

      "Tags": [ "train", "b2b", "production" ]
    }

  }


}

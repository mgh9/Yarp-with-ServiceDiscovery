{
  "Logging": {
    "Console": {
      "TimestampFormat": "[yyyy-MM-dd HH:mm:ss] "
    },

    "LogLevel": {
      "Default": "Debug",
      
      "Microsoft.AspNetCore": "Information",
      "Microsoft.AspNetCore.Routing.*": "Warning",

      "Yarp": "Trace"
    }
  },

  "ConsulServiceDiscovery": {
    "ConsulClient": {
      "Host": "http://consul-dev-server1:8500",
      "Datacenter": "yarp_dc"
    },

    "AutoDiscovery": {
      "IsEnabled": true,
      "IntervalSeconds": 30
    }
  },

  "CentralSwagger": {
    "IsEnabled": true
  }

}

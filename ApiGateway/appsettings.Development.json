{
  "Logging": {
    "Console": {
      "TimestampFormat": "[yyyy-MM-dd HH:mm:ss] "
    },
    "LogLevel": {
      "Default": "Debug",
      
      "Microsoft.AspNetCore": "Information",
      "Microsoft.AspNetCore.Routing.*": "Warning",

      "Yarp": "Information"
    }
  },

  "ConsulServiceDiscovery": {
    "ConsulClient": {
      "Host": "http://localhost:8500",
      "Datacenter": "yarp_dc"
    },

    "ReloadRoutesAndClusters": {
      "IntervalSeconds": 30
    }

  }
}

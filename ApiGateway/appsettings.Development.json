{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "ConsulServiceDiscovery": {
    "ConsulClient": {
      "Host": "http://localhost:8500",
      "Datacenter": "yarp_dc"
    },
    "AutoDiscovery": {
      "IsEnabled": true,
      "IntervalSeconds": 30
    }
  },

  "CentralSwagger": {
    "IsEnabled": true
  }

}
